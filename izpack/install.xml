<?xml version="1.0" encoding="iso-8859-1" standalone="yes"?>
<installation version="5.0">
    <info>
        <appname>SPEDTeste</appname>
        <appversion>1.0.1</appversion>
        <authors>
            <author email="otojunior@gmail.com" name="Oto Junior"/>
        </authors>
        <uninstaller name="Uninstaller.jar" write="yes"/>
        <requiresjdk>no</requiresjdk>
        <writeinstallationinformation>no</writeinstallationinformation>
        <!-- <pack200/> -->
        <run-privileged condition="izpack.windowsinstall.vista|izpack.windowsinstall.7"/>
    </info>
    <guiprefs height="480" resizable="no" width="640">
        <modifier key="useFlags" value="yes"/>
        <modifier key="langDisplayType" value="default"/>
    </guiprefs>
    <locale>
        <langpack iso3="bra"/>
    </locale>
    <panels>
        <panel classname="CheckedHelloPanel"/>
        <panel classname="TargetPanel"/>
        <panel classname="InstallPanel"/>
        <panel classname="SimpleFinishPanel"/>
    </panels>
    <packs>
        <pack name="SPED" preselected="yes" required="yes">
            <description>Pacote principal</description>
            <file override="update" src="../target/sample-quickstart-1.0-SNAPSHOT.jar" targetdir="$INSTALL_PATH/"/>
            <fileset override="update" includes="recursos/**/*" dir=".." targetdir="$INSTALL_PATH/"/>
        </pack>
    </packs>
</installation>
